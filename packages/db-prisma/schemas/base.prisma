generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
  runtime  = "bun"
}

datasource db {
  provider = "postgresql"
  schemas  = ["public", "auth"]
}

generator prismabox {
  provider                    = "prismabox"
  output                      = "../generated/prismabox"
  typeboxImportVariableName   = "t"
  typeboxImportDependencyName = "elysia"
  importFileExtension         = ".js"
}
